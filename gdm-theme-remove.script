#!/bin/bash

#Used directory
gnomedir=/usr/share/gnome-shell
themedir=/usr/share/themes/Adapta-Breath/gnome-shell
theme=gnome-shell-theme.gresource

#Remove GDM theme

cd $gnomedir
if [ -f $theme.old ]; then
   rm $theme
   mv $theme.old $theme
else
   echo "Backupped GDM theme not present"
fi

exit 0
