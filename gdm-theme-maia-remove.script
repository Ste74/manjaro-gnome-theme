#!/bin/bash

#Used directory

gnomedir=/usr/share/gnome-shell
theme=gnome-shell-theme.gresource
theme_dir=/usr/share/themes/manjaro-gdm-theme

#Remove GDM theme and icons

cd $gnomedir
if [ -f $theme.old ]; then
   rm $theme
   mv $theme.old $theme
else
   echo
   echo "Backup GDM theme not present: re-install gnome-shell is needed"
   echo
fi

cd $theme_dir
   rm -Rf *.svg *.png
   